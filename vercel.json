{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node@latest"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    { "src": "/(.+\\.[a-z]+)$", "dest": "public/$1" }
  ]
}
