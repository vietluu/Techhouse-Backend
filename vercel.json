{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/public" },
    { "src": "/(.*)", "dest": "/public" },
    {
      "src": "/api/v1/(.*)",
      "dest": "src/routes/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
